<Window x:Class="WvW_Status.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WvW_Status"
        mc:Ignorable="d"
        Title="Guild Wars 2 WvW Status" Height="100" Width="700" Background="black" FontFamily="Cambria" ResizeMode="CanMinimize">
    <Grid>
        <RadioButton x:Name="radioButtonNA" Content="NA Servers" Foreground="WhiteSmoke" FontFamily="Cambria" FontSize ="16" HorizontalAlignment="Left" Margin="20,20,0,0" VerticalAlignment="Top" IsChecked="True"/>
        <RadioButton x:Name="radioButtonEU" Content="EU Servers" Foreground="WhiteSmoke" FontFamily="Cambria" FontSize ="16" HorizontalAlignment="Left" Margin="120,20,0,0" VerticalAlignment="Top"/>        
    </Grid>
</Window>
